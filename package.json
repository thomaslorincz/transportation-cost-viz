{
  "name": "transportation-cost-viz",
  "version": "2.1.0",
  "description": "A visualization of household transportation cost data in Edmonton, Alberta",
  "private": true,
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "build:dev": "rimraf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "dev": "npm run build:dev && npm start",
    "build:prod": "rimraf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "prod": "npm run build:prod && npm start",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaslorincz/transportation-cost-viz.git"
  },
  "author": "City of Edmonton - Systems Analytics",
  "contributors": [
    {
      "name": "Thomas Lorincz",
      "email": "thomas.lorincz@edmonton.ca",
      "url": "https://github.com/thomaslorincz"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thomaslorincz/transportation-cost-viz/issues"
  },
  "homepage": "https://github.com/thomaslorincz/transportation-cost-viz#readme",
  "dependencies": {
    "d3-fetch": "^1.1.2",
    "deck.gl": "^7.3.10",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.5",
    "helmet": "^3.21.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-map-gl": "^5.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/mapbox-gl": "^1.6.1",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "compression-webpack-plugin": "^3.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "style-loader": "^1.1.1",
    "terser-webpack-plugin": "^2.3.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
}
